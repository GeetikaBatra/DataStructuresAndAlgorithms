package DataStructures;

public class Queue2Stacks {
	
	StackUsingArray s1= new StackUsingArray();
	StackUsingArray s2= new StackUsingArray(0);
	public int size() {
		if(s1.size()==0 || s2.size()==0)
		{
			return 0;
		}
		else if(s1.size()!=0)
		{
			return s1.size(); 
		}
		else
		{
			return s2.size();
		}
		
		
	}
	public int top()
	{
		return s2.t
	}
}
